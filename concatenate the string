Method;

import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner mv=new Scanner(System.in);
        String x=mv.nextLine().trim();
        String y=mv.nextLine().trim();
        System.out.print(x+y); 
    }
}


