import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner mv=new Scanner(System.in);
        int size=mv.nextInt();
        int sum=0;
        int arr[]=new int[size];
        for(int i=0;i<size;i++)
        {
            arr[i]=mv.nextInt();
        }
        for(int i=0;i<size;i++){
            if(arr[i]>0){
                 sum+=arr[i]*arr[i];  
            }
        }
            System.out.println(sum);
    }
}
