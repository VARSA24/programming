import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner mv=new Scanner(System.in);
        int a=mv.nextInt();
        int s=0,r;
        if(a<99){
            System.out.println("Invalid Input");
            return;
        }
        while(a!=0)
        {
            r=a%10;
            s=s+r;
            a=a/10;
        }
          System.out.println("Sum of digit is "+s); 
        }
             
    }
