import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
       Scanner mv=new Scanner(System.in);
        int x=mv.nextInt();
        if(x<1||x>9)
        {
           System.out.println("Invalid Input");
            return;
        }
        System.out.println("Square of "+x+" is "+(x*x));       
    }
    class Square
    {
        int Square(int n)
        {
            return(n*n);
        }
    }
}
