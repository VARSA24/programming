import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
       Scanner mv=new Scanner(System.in);
        int n=mv.nextInt();
for(int i=0;i<n;i++)
{
   char ch=(char)('A'+i);
            for(int j=0;j<=i;j++)
            {
                System.out.print((char)(ch+j)+" ");          
            }
            System.out.println();
            }
        }      
    }
