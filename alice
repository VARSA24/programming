import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner mv=new Scanner(System.in);
        {
            int a=mv.nextInt();
            int b=a/10;
            int c=a%10;
            int d=b+c;
            System.out.println("Bird said"+":"+a);
            System.out.println("Alice must go in path"+"-"+d);
        }
    }
}
